{
  "name": "@covia/covia-sdk",
  "version": "1.0.0",
  "description": "Typescript library for covia-ai",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "prepublishOnly": "npm run build && npm test",
    "prepare": "npm run build"
  },
  "keywords": [
    "covia",
    "grid",
    "api",
    "typescript",
    "data-assets",
    "operations"
  ],
  "author": "Covia AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/covia-ai/covia-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/covia-ai/covia-sdk/issues"
  },
  "homepage": "https://github.com/covia-ai/covia-sdk#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "did-resolver": "^4.1.0",
    "web-did-resolver": "^2.0.32"
  }
}